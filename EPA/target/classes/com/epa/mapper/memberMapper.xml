<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epa.mapper.memberMapper">
    
    <select id="memberLogin" parameterType="com.epa.entity.epa"
        resultType="com.epa.entity.epa">
        select * from MEM_TB where MEM_ID=#{MEM_ID} and MEM_PW=#{MEM_PW}
    </select>

    <insert id="memberJoin">
      insert into MEM_TB(MEM_EMAIL,MEM_ID,MEM_PW,MEM_NM,MEM_H,MEM_W,MEM_AGE,MEM_G) values (#{MEM_EMAIL},#{MEM_ID},#{MEM_PW},#{MEM_NM},#{MEM_H},#{MEM_W},#{MEM_AGE},#{MEM_G})
    </insert>

</mapper>